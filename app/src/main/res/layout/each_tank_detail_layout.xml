<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>
        <variable
            name="boundTankDetails"
            type="com.newage.plantedaqua.models.TanksDetails" />

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.cardview.widget.CardView
            app:cardCornerRadius="2dp"
            android:id="@+id/cardView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="2dp"
            android:layout_marginLeft="2dp"
            android:layout_marginTop="2dp"
            android:layout_marginEnd="2dp"
            android:layout_marginRight="2dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_height="wrap_content"
                android:layout_width="match_parent">


                <TextView
                    android:clickable="true"
                    android:focusable="true"
                    android:text="@{boundTankDetails.tankName}"
                    android:id="@+id/TankNameText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:textColor="@color/colorAccent"
                    android:textSize="25sp"
                    android:textStyle="italic|bold"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.498"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:text="Tank 1" />

                <ImageView
                    android:id="@+id/deleteTank"
                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    android:layout_marginEnd="20dp"
                    android:layout_marginRight="20dp"
                    android:clickable="true"
                    android:focusable="true"
                    android:visibility="gone"
                    android:onClick="onTankDashBoardItemsClicked"
                    android:tag="@{boundTankDetails.tankID}"
                    app:layout_constraintEnd_toStartOf="@+id/resetImage"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:srcCompat="@drawable/delete_values"
                    tools:layout_editor_absoluteY="0dp" />

                <ImageView
                    android:id="@+id/tankOptionsImage"
                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    android:layout_marginEnd="16dp"
                    android:layout_marginRight="16dp"
                    android:clickable="true"
                    android:focusable="true"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/deleteTank"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@drawable/options_menu" />

                <ImageView
                    android:id="@+id/resetImage"
                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    android:layout_marginEnd="16dp"
                    android:layout_marginRight="16dp"
                    android:visibility="gone"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@drawable/cross" />
                <ImageView
                    android:id="@+id/editImage"
                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    android:layout_marginEnd="20dp"
                    android:layout_marginRight="20dp"
                    android:visibility="gone"
                    android:onClick="onTankDashBoardItemsClicked"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/deleteTank"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@drawable/edit_tanks" />
            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.cardview.widget.CardView>

        <ScrollView
            android:id="@+id/scrollView2"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/cardView">

            <LinearLayout

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"

                >

                <androidx.cardview.widget.CardView
                    android:id="@+id/cardView2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="2dp"
                    app:cardBackgroundColor="@color/customDarkBackground"
                    app:cardCornerRadius="2dp"
                   >

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:padding="6dp">


                        <ImageView

                            android:id="@+id/imageView"
                            android:layout_width="150dp"
                            android:layout_height="100dp"
                            android:layout_margin="1dp"
                            android:adjustViewBounds="true"
                            android:contentDescription="@string/tank_image"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            tools:srcCompat="@tools:sample/backgrounds/scenic" />

                        <TextView
                            android:id="@+id/typeText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:layout_marginLeft="10dp"
                            android:text="@{boundTankDetails.tankType}"
                            android:textColor="@color/highlight"
                            android:textSize="12sp"
                            android:textStyle="italic"
                            app:layout_constraintBottom_toTopOf="@+id/statusText"
                            app:layout_constraintStart_toEndOf="@+id/imageView"
                            app:layout_constraintTop_toBottomOf="@+id/lightZoneText"
                            app:visibleGone="@{boundTankDetails.tankType.empty}"
                            tools:text="Planted" />

                        <TextView
                            android:id="@+id/statusText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:layout_marginLeft="10dp"
                            android:text="@{boundTankDetails.tankStatus}"
                            android:textColor="@color/highlight"
                            android:textSize="12sp"
                            android:textStyle="italic"
                            app:layout_constraintBottom_toTopOf="@+id/co2SupplyText"
                            app:layout_constraintStart_toEndOf="@+id/imageView"
                            app:layout_constraintTop_toBottomOf="@+id/typeText"
                            app:visibleGone="@{boundTankDetails.tankStatus.empty}"
                            tools:text="In Use" />

                        <TextView
                            android:id="@+id/fromDateText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:layout_marginLeft="10dp"
                            android:text="@{`From `+boundTankDetails.tankStartDate}"
                            android:textColor="@color/highlight"
                            android:textSize="12sp"
                            android:textStyle="italic"
                            app:layout_constraintBottom_toTopOf="@+id/toDateText"
                            app:layout_constraintStart_toEndOf="@+id/imageView"
                            app:layout_constraintTop_toBottomOf="@+id/co2SupplyText"
                            app:visibleGone="@{boundTankDetails.tankStartDate.empty}"
                            tools:text="From 9/11/2019" />

                        <TextView
                            android:id="@+id/toDateText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:layout_marginLeft="10dp"
                            android:text="@{`To `+boundTankDetails.tankEndDate}"
                            android:textColor="@color/highlight"
                            android:textSize="12sp"
                            android:textStyle="italic"
                            app:layout_constraintBottom_toTopOf="@+id/constraintLayoutEachTankOptions"
                            app:layout_constraintStart_toEndOf="@+id/imageView"
                            app:layout_constraintTop_toBottomOf="@+id/fromDateText"
                            app:visibleGone="@{boundTankDetails.tankEndDate.empty}"
                            tools:text="To 10/4/2020" />

                        <TextView
                            android:id="@+id/co2SupplyText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:layout_marginLeft="10dp"
                            android:text="@{boundTankDetails.tankCO2Supply +` CO2`}"
                            android:textColor="@color/highlight"
                            android:textSize="12sp"
                            android:textStyle="italic"
                            app:layout_constraintBottom_toTopOf="@+id/fromDateText"
                            app:layout_constraintStart_toEndOf="@+id/imageView"
                            app:layout_constraintTop_toBottomOf="@+id/statusText"
                            app:visibleGone="@{boundTankDetails.tankCO2Supply.empty}"
                            tools:text="Liquid CO2" />

                        <TextView
                            android:id="@+id/lightZoneText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:layout_marginLeft="10dp"
                            android:text="@{boundTankDetails.tankLightRegion}"
                            android:textColor="@color/highlight"
                            android:textSize="12sp"
                            android:textStyle="italic"
                            app:layout_constraintBottom_toTopOf="@+id/typeText"
                            app:layout_constraintStart_toEndOf="@+id/imageView"
                            app:layout_constraintTop_toTopOf="parent"
                            app:visibleGone="@{boundTankDetails.tankLightRegion.empty}"
                            tools:text="Medium Light" />

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/constraintLayoutEachTankOptions"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:visibility="gone"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/imageView">

                            <ImageView
                                android:id="@+id/imageNutrientOption"
                                android:layout_width="35dp"
                                android:layout_height="35dp"
                                android:clickable="true"
                                android:focusable="true"
                                android:onClick="onTankDashBoardItemsClicked"
                                app:layout_constraintBottom_toBottomOf="@+id/imageLogOptions"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintHorizontal_bias="0.5"
                                app:layout_constraintStart_toEndOf="@+id/imageFaunaOption"
                                app:layout_constraintTop_toTopOf="@+id/imageLogOptions"
                                app:srcCompat="@drawable/chart" />

                            <ImageView
                                android:id="@+id/imageFloraOption"
                                android:layout_width="35dp"
                                android:layout_height="35dp"
                                android:clickable="true"
                                android:focusable="true"
                                android:onClick="onTankDashBoardItemsClicked"
                                app:layout_constraintBottom_toBottomOf="@+id/imageLogOptions"
                                app:layout_constraintEnd_toStartOf="@+id/imageFaunaOption"
                                app:layout_constraintHorizontal_bias="0.5"
                                app:layout_constraintStart_toEndOf="@+id/imageTanksEquipmentOption"
                                app:layout_constraintTop_toTopOf="@+id/imageLogOptions"
                                app:srcCompat="@drawable/flora" />

                            <ImageView
                                android:id="@+id/imageFaunaOption"
                                android:layout_width="35dp"
                                android:layout_height="35dp"
                                android:clickable="true"
                                android:focusable="true"
                                android:onClick="onTankDashBoardItemsClicked"
                                app:layout_constraintBottom_toBottomOf="@+id/imageLogOptions"
                                app:layout_constraintEnd_toStartOf="@+id/imageNutrientOption"
                                app:layout_constraintHorizontal_bias="0.5"
                                app:layout_constraintStart_toEndOf="@+id/imageFloraOption"
                                app:layout_constraintTop_toTopOf="@+id/imageLogOptions"
                                app:srcCompat="@drawable/fish" />

                            <ImageView
                                android:id="@+id/imageTanksEquipmentOption"
                                android:layout_width="35dp"
                                android:layout_height="35dp"
                                android:clickable="true"
                                android:focusable="true"
                                android:onClick="onTankDashBoardItemsClicked"
                                app:layout_constraintBottom_toBottomOf="@+id/imageLogOptions"
                                app:layout_constraintEnd_toStartOf="@+id/imageFloraOption"
                                app:layout_constraintHorizontal_bias="0.5"
                                app:layout_constraintStart_toEndOf="@+id/imageLogOptions"
                                app:layout_constraintTop_toTopOf="@+id/imageLogOptions"
                                app:srcCompat="@drawable/equipment" />

                            <ImageView
                                android:id="@+id/imageLogOptions"
                                android:layout_width="35dp"
                                android:layout_height="35dp"
                                android:layout_marginTop="10dp"
                                android:layout_marginBottom="8dp"
                                android:clickable="true"
                                android:focusable="true"
                                android:onClick="onTankDashBoardItemsClicked"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintEnd_toStartOf="@+id/imageTanksEquipmentOption"
                                app:layout_constraintHorizontal_bias="0.5"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toBottomOf="@+id/imageMacroOption"
                                app:srcCompat="@drawable/log" />

                            <ImageView
                                android:id="@+id/imageTaskOptions"
                                android:layout_width="35dp"
                                android:layout_height="35dp"
                                android:clickable="true"
                                android:focusable="true"
                                android:onClick="onTankDashBoardItemsClicked"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintHorizontal_bias="0.5"
                                app:layout_constraintStart_toEndOf="@+id/imageTPAOptions"
                                app:layout_constraintTop_toTopOf="@+id/imageMacroOption"
                                app:srcCompat="@drawable/tasks" />

                            <ImageView
                                android:id="@+id/imageLightOption"
                                android:layout_width="35dp"
                                android:layout_height="35dp"
                                android:clickable="true"
                                android:focusable="true"
                                android:onClick="onTankDashBoardItemsClicked"
                                app:layout_constraintEnd_toStartOf="@+id/imageTPAOptions"
                                app:layout_constraintHorizontal_bias="0.5"
                                app:layout_constraintStart_toEndOf="@+id/imageMicroOption"
                                app:layout_constraintTop_toTopOf="@+id/imageMacroOption"
                                app:srcCompat="@drawable/light" />

                            <ImageView
                                android:id="@+id/imageMacroOption"
                                android:layout_width="35dp"
                                android:layout_height="35dp"
                                android:layout_marginTop="8dp"
                                android:clickable="true"
                                android:focusable="true"
                                android:onClick="onTankDashBoardItemsClicked"
                                app:layout_constraintEnd_toStartOf="@+id/imageMicroOption"
                                app:layout_constraintHorizontal_bias="0.5"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent"
                                app:srcCompat="@drawable/table" />

                            <ImageView
                                android:id="@+id/imageMicroOption"
                                android:layout_width="35dp"
                                android:layout_height="35dp"
                                android:clickable="true"
                                android:focusable="true"
                                android:onClick="onTankDashBoardItemsClicked"
                                app:layout_constraintEnd_toStartOf="@+id/imageLightOption"
                                app:layout_constraintHorizontal_bias="0.5"
                                app:layout_constraintStart_toEndOf="@+id/imageMacroOption"
                                app:layout_constraintTop_toTopOf="@+id/imageMacroOption"
                                app:srcCompat="@drawable/macrodose" />

                            <ImageView
                                android:id="@+id/imageTPAOptions"
                                android:layout_width="35dp"
                                android:layout_height="35dp"
                                android:clickable="true"
                                android:focusable="true"
                                android:onClick="onTankDashBoardItemsClicked"
                                app:layout_constraintEnd_toStartOf="@+id/imageTaskOptions"
                                app:layout_constraintHorizontal_bias="0.5"
                                app:layout_constraintStart_toEndOf="@+id/imageLightOption"
                                app:layout_constraintTop_toTopOf="@+id/imageMacroOption"
                                app:srcCompat="@drawable/album" />
                        </androidx.constraintlayout.widget.ConstraintLayout>


                    </androidx.constraintlayout.widget.ConstraintLayout>
                </androidx.cardview.widget.CardView>


                <androidx.cardview.widget.CardView
                    android:id="@+id/dosageInfoCard"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="2dp"
                    app:cardCornerRadius="8dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:clickable="true"
                            android:focusable="true"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <TextView
                                android:id="@+id/textView5"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="start"
                                android:gravity="center"
                                android:padding="3dp"
                                android:text="@string/dosage_recommendation"
                                android:textColor="@color/colorPrimaryDark"
                                android:textSize="15sp"
                                android:textStyle="bold"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent" />

                            <ImageView
                                android:adjustViewBounds="true"
                                android:scaleType="fitXY"
                                android:id="@+id/showDosage"
                                android:clickable="true"
                                android:focusable="true"
                                android:layout_width="30dp"
                                android:layout_height="30dp"
                                android:layout_marginEnd="16dp"
                                android:layout_marginRight="16dp"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintTop_toTopOf="parent"
                                app:srcCompat="@drawable/dropdown" />

                        </androidx.constraintlayout.widget.ConstraintLayout>

                        <LinearLayout
                            android:visibility="gone"
                            android:id="@+id/dosageInfoLayout"
                            android:layout_width="match_parent"
                            android:orientation="vertical"
                            android:layout_height="wrap_content">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:orientation="horizontal"
                            android:layout_height="wrap_content">


                        <TextView

                            android:layout_width="50dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:padding="3dp"
                            android:layout_marginStart="3dp"
                            android:layout_marginLeft="3dp"
                            android:text="@string/micro"
                            android:textColor="@color/colorPrimary"
                            android:textSize="15sp"
                            android:textStyle="bold|italic" />
                        <ImageView
                            android:onClick="onTankDashBoardItemsClicked"
                            android:clickable="true"
                            android:focusable="true"
                            android:id="@+id/SetMicroValue"
                            android:layout_gravity="center"
                            android:layout_width="wrap_content"
                            android:textSize="12sp"
                            android:src="@drawable/edit_tanks"
                            android:layout_height="match_parent"/>
                        </LinearLayout>
                        <TextView
                            android:layout_marginStart="3dp"
                            android:layout_marginLeft="3dp"
                            android:id="@+id/DosageMicro"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="start"
                            android:padding="3dp"
                            android:text="@string/no_data_for_micro_dosage"
                            android:textColor="@color/grey_500"
                            android:textSize="12sp" />
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:orientation="horizontal"
                        android:layout_height="wrap_content">

                        <TextView
                            android:layout_marginStart="3dp"
                            android:layout_marginLeft="3dp"
                            android:layout_width="50dp"
                            android:layout_height="wrap_content"
                           android:layout_gravity="center"
                            android:padding="3dp"
                            android:text="@string/macro"
                            android:textColor="@color/colorPrimary"
                            android:textSize="15sp"
                            android:textStyle="bold|italic" />

                        <ImageView
                            android:onClick="onTankDashBoardItemsClicked"
                            android:clickable="true"
                            android:focusable="true"
                            android:id="@+id/SetMacroValue"
                            android:layout_gravity="center"
                            android:layout_width="wrap_content"
                            android:textSize="12sp"
                            android:src="@drawable/edit_tanks"
                            android:layout_height="match_parent"/>
                    </LinearLayout>
                        <TextView
                            android:layout_marginStart="3dp"
                            android:layout_marginLeft="3dp"
                            android:id="@+id/DosageMacro"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="start"
                            android:padding="3dp"
                            android:text="@string/no_data_for_macro_dose"
                            android:textColor="@color/grey_500"
                            android:textSize="12sp" />
                    </LinearLayout>
                    </LinearLayout>

                </androidx.cardview.widget.CardView>


                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="2dp"
                    app:cardCornerRadius="8dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/PendingDashBoardTasks"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:padding="3dp"
                            android:text="@string/pending_tasks"
                            android:textColor="@color/duskYellow"
                            android:textSize="15sp"
                            android:textStyle="bold" />


                        <LinearLayout
                            android:id="@+id/LinearDashBoardPending"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="2dp"
                            android:background="@drawable/ok_button_background"
                            android:padding="3dp">

                            <androidx.recyclerview.widget.RecyclerView
                                android:id="@+id/PendingDashBoard_RecyclerView"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:minHeight="50dp">

                            </androidx.recyclerview.widget.RecyclerView>

                        </LinearLayout>
                    </LinearLayout>
                </androidx.cardview.widget.CardView>


                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="2dp"
                    app:cardCornerRadius="8dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/UpcomingDashBoardTasks"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:padding="3dp"
                            android:text="@string/upcoming_tasks"
                            android:textColor="@color/colorAccent"
                            android:textSize="15sp"
                            android:textStyle="bold" />


                        <LinearLayout
                            android:id="@+id/LinearDashBoardUpcoming"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="2dp"
                            android:background="@drawable/ok_button_background"
                            android:padding="3dp">

                            <androidx.recyclerview.widget.RecyclerView
                                android:id="@+id/UpcomingDashBoard_RecyclerView"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:minHeight="50dp">

                            </androidx.recyclerview.widget.RecyclerView>

                        </LinearLayout>
                    </LinearLayout>
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="2dp"
                    android:layout_marginTop="5dp"
                    app:cardCornerRadius="8dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <TextView

                            android:id="@+id/RecommendationsDashBoard"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:padding="3dp"
                            android:text="@string/recommendations"
                            android:textColor="@color/colorPrimary"
                            android:textSize="15sp"
                            android:textStyle="bold" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:padding="3dp"
                            android:textSize="12sp"
                            android:text="@string/suggested_reco"
                            android:textStyle="italic" />


                        <LinearLayout
                            android:id="@+id/LinearRecoDashBoard"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="2dp"
                            android:background="@drawable/ok_button_background"
                            android:padding="3dp">

